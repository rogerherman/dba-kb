	<query>
		<sql><![CDATA[SELECT to_char(r.start_time, 'MM/DD/YYYY') AS backup_date,
       round(r.elapsed_seconds / 60, 2) AS backup_time,
       round(r.output_bytes_per_sec / 1024 / 1024, 2) AS mb_per_sec
  FROM v$rman_backup_job_details r
 WHERE r.input_type = 'DB FULL'
 ORDER BY backup_date]]></sql>
