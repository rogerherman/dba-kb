	<query>
		<sql><![CDATA[SELECT to_date(log_time, 'MM-DD-YYYY HH24') AS switch_time, switches_reported, 4 AS switch_threshold
  FROM (SELECT to_char(lh.first_time, 'MM-DD-YYYY HH24') AS log_time,
               COUNT(*) AS switches_reported
          FROM v$log_history lh
         WHERE lh.first_time >= TRUNC(sysdate -14)
         GROUP BY to_char(lh.first_time, 'MM-DD-YYYY HH24'))
 ORDER BY switch_time]]></sql>
	</query>
